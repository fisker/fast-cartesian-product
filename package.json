{
  "name": "fast-cartesian-product",
  "description": "cheap and fast cartesian-product",
  "version": "0.0.2",
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "browser": "lib/index.min.js",
  "unpkg": "lib/index.min.js",
  "jsdelivr": "lib/index.min.js",
  "repository": "fisker/fast-cartesian-product",
  "homepage": "https://github.com/fisker/fast-cartesian-product#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/preset-env": "7.5.5",
    "@commitlint/cli": "8.1.0",
    "@fisker/commitlint-config": "1.1.0",
    "@fisker/eslint-config": "1.2.8",
    "@fisker/husky-config": "1.1.0",
    "@fisker/lint-staged-config": "1.0.4",
    "@fisker/prettier-config": "1.0.18",
    "ava": "2.2.0",
    "cz-conventional-changelog-emoji": "0.1.0",
    "eslint": "6.1.0",
    "eslint-config-fisker": "4.5.0",
    "esm": "3.2.25",
    "husky": "3.0.1",
    "lint-staged": "9.2.1",
    "markdownlint-cli": "0.17.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.18.2",
    "rimraf": "2.6.3",
    "rollup": "1.17.0",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-commonjs": "10.0.1",
    "rollup-plugin-filesize": "6.1.1",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-prettier": "0.6.0",
    "rollup-plugin-terser": "5.1.1"
  },
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog-emoji"
    }
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "dist": "run-p dist:*",
    "dist:npm": "np --yolo --no-yarn",
    "build": "run-p build:*",
    "build:js": "rollup --config",
    "format": "run-p format:*",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "prettier \"**/*.{css,html,js,json,less,md,scss,ts,vue,yaml,yml}\" --write",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint \"**/*.{js,mjs,vue}\"",
    "lint:markdown": "markdownlint \"**/*.md\" --ignore \"**/node_modules/**\"",
    "lint:prettier": "prettier \"**/*.{css,html,js,json,less,md,scss,ts,vue,yaml,yml}\" --check",
    "test": "ava",
    "test-coverage": "nyc yarn test",
    "clean": "rimraf lib",
    "release": "run-s clean build format test dist"
  },
  "author": {
    "name": "fisker Cheung",
    "email": "lionkay@gmail.com",
    "url": "https://www.fiskercheung.com/"
  },
  "sideEffects": false,
  "nyc": {
    "require": "esm"
  },
  "ava": {
    "verbose": true,
    "require": "esm"
  }
}
