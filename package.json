{
  "name": "fast-cartesian-product",
  "description": "fast cartesian product",
  "version": "1.0.0",
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "browser": "lib/index.min.js",
  "unpkg": "lib/index.min.js",
  "jsdelivr": "lib/index.min.js",
  "repository": "fisker/fast-cartesian-product",
  "homepage": "https://github.com/fisker/fast-cartesian-product#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "keywords": [
    "cartesian",
    "product",
    "cartesian-product",
    "sku"
  ],
  "devDependencies": {
    "@babel/core": "7.6.2",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/preset-env": "7.6.2",
    "@commitlint/cli": "8.2.0",
    "@fisker/commitlint-config": "1.1.3",
    "@fisker/eslint-config": "1.3.3",
    "@fisker/husky-config": "1.1.3",
    "@fisker/lint-staged-config": "1.0.5",
    "@fisker/prettier-config": "1.0.21",
    "ava": "2.4.0",
    "babel-plugin-transform-async-to-promises": "0.8.14",
    "benchmark": "2.1.4",
    "benchmarkify": "2.1.2",
    "cp-file": "7.0.0",
    "cz-conventional-changelog": "3.0.2",
    "eslint": "6.5.1",
    "esm": "3.2.25",
    "husky": "3.0.7",
    "lint-staged": "9.4.1",
    "markdownlint-cli": "0.18.0",
    "npm-run-all": "4.1.5",
    "nyc": "14.1.1",
    "prettier": "1.18.2",
    "pretty-format": "24.9.0",
    "rimraf": "3.0.0",
    "rollup": "1.22.0",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-filesize": "6.2.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-prettier": "0.6.0",
    "rollup-plugin-terser": "5.1.2"
  },
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "benchmark": "node -r esm benchmark",
    "benchmark2": "node -r esm benchmark/benchmarkify.js",
    "build": "run-p build:*",
    "build:js": "rollup --config",
    "clean": "rimraf lib",
    "dist": "run-p dist:*",
    "dist:npm": "np --yolo --no-yarn",
    "format": "run-p format:*",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "prettier \"**/*.{css,html,js,json,less,md,scss,ts,vue,yaml,yml}\" --write",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint \"**/*.{js,mjs,vue}\"",
    "lint:markdown": "markdownlint \"**/*.md\" --ignore \"**/node_modules/**\"",
    "lint:prettier": "prettier \"**/*.{css,html,js,json,less,md,scss,ts,vue,yaml,yml}\" --check",
    "release": "run-s clean build format test dist",
    "test": "ava",
    "test-coverage": "nyc ava"
  },
  "author": {
    "name": "fisker Cheung",
    "email": "lionkay@gmail.com",
    "url": "https://www.fiskercheung.com/"
  },
  "sideEffects": false,
  "nyc": {
    "require": "esm",
    "reporter": [
      "lcov",
      "text"
    ],
    "include": [
      "src/**"
    ]
  },
  "ava": {
    "verbose": true,
    "require": "esm",
    "helpers": [
      "test/helpers/**"
    ]
  }
}
